# RADIUS Database Configuration
RADIUS_DB_CONNECTION=radius
RADIUS_DB_HOST=radius-db
RADIUS_DB_PORT=3306
RADIUS_DB_DATABASE=radius
RADIUS_DB_USERNAME=radius
RADIUS_DB_PASSWORD=radius_secret
